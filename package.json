{
  "name": "singlestore-drizzle-orm",
  "version": "1.0.0",
  "description": "Template project for open source projects from SingleStore",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "postinstall": "ln -s @drodrigues4/drizzle-orm node_modules/drizzle-orm",
    "test": "vitest --no-file-parallelism",
    "start": "npx tsx src/index.ts",
    "generate": "npx drizzle-kit generate --config drizzle.config.ts",
    "migrate": "npx tsx migrate.ts",
    "build": "tsc && copyfiles -u 1 'src/public/*' 'src/public/**/*' dist/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/singlestore-labs/singlestore-drizzle-orm.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/singlestore-labs/singlestore-drizzle-orm/issues"
  },
  "homepage": "https://github.com/singlestore-labs/singlestore-drizzle-orm#readme",
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.21",
    "@types/helmet": "4.0.0",
    "@types/http-status-codes": "1.2.0",
    "@types/uuid": "10.0.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "0.23.0",
    "tsx": "4.16.2",
    "vitest": "2.0.4"
  },
  "dependencies": {
    "@drodrigues4/drizzle-orm": "latest",
    "bson": "^6.8.0",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "mysql2": "3.10.3",
    "ts-node": "10.9.2",
    "typescript": "5.5.3"
  }
}
